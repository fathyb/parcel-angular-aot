{
    "name": "angular-test",
    "version": "1.0.0",
    "license": "MIT",
    "description": "",
    "repository": "",
    "contributors": [],
    "main": "./main.electron.js",
    "scripts": {
        "clean": "rm -rf node_modules ngfactory doc build && npm cache clean",
        "webdriver:update": "webdriver-manager update",
        "webdriver:start": "webdriver-manager start",
        "lint": "tslint --force \"src/app/**/*.ts\"",
        "e2e": "protractor protractor.config.js",
        "e2e:live": "protractor protractor.config.js --elementExplorer",
        "pretest": "",
        "test": "karma start karma.conf.js",
        "test:watch": "karma start karma.conf.js --no-single-run --auto-watch",
        "ci": "npm run e2e && npm run test",
        "ci:watch": "npm run e2e && npm run test:watch",
        "start": "yarn serve",
        "postinstall": "",
        "build": "rm -rf dist .cache &&  node_modules/parcel-bundler/bin/cli.js build index.html --public-url / --no-minify && cp -R src/public/assets dist/assets",
        "copy-assets": "rm -rf dist && mkdir dist && cp -R src/public/assets dist/assets",
        "serve": "rm -rf .cache && yarn copy-assets && node_modules/parcel-bundler/bin/cli.js index.html --no-hmr"
    },
    "dependencies": {
        "@angular/animations": "^5.0.0",
        "@angular/common": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "@angular/http": "^5.0.0",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/platform-server": "^5.0.0",
        "@angular/router": "^5.0.0",
        "core-js": "^2.5.1",
        "http-server": "^0.10.0",
        "reflect-metadata": "~0.1.10",
        "rxjs": "^5.5.2",
        "systemjs": "0.20.14",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "@angular/compiler": "^5.0.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/language-service": "^5.0.0",
        "@types/jasmine": "2.5.53",
        "@types/jasminewd2": "^2.0.3",
        "@types/karma": "1.7.1",
        "@types/node": "8.0.2",
        "codelyzer": "^4.0.1",
        "jasmine-core": "^2.8.0",
        "jasmine-spec-reporter": "^4.2.0",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.1",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-remap-coverage": "^0.1.4",
        "parcel-bundler": "git+https://github.com/fathyb/parcel.git#fix/dynamic-imports",
        "parcel-plugin-typescript": "0.3.0-angular-aot.13",
        "protractor": "^5.1.2",
        "ts-helpers": "^1.1.2",
        "ts-node": "^3.3.0",
        "tslib": "^1.6.1",
        "tslint": "^5.4.3",
        "typescript": "2.4.2"
    }
}
